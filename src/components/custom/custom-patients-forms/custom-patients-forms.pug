.custom-patients-forms
  .custom-patients-forms__container
    .custom-patients-forms__col
      .custom-patients-forms__title(v-html="props.title")
      .custom-patients-forms__button-row
        a.custom-patients-forms__button(v-for="button in props.forms" v-html="button.label", :href="button.link")
    .custom-patients-forms__col.custom-patients-forms__col--photo(:style="{'background': 'url(' + props.background_img + ') no-repeat center / cover'}")


